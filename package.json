{
  "name": "react-typescript-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "intl-messageformat-parser": "^6.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.21.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --verbose true",
    "test-verbose": "react-scripts test --watchAll --verbose true",
    "test-unmocked": "TEST_TYPE=unmocked react-scripts test --watchAll --silent --env=jest-environment-jsdom-sixteen --verbose true",
    "test-unmocked-verbose": "TEST_TYPE=unmocked react-scripts test --watchAll --env=jest-environment-jsdom-sixteen --verbose true",
    "test-coverage": "react-scripts test  --watchAll --collectCoverage --silent --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "compile-i18n": "npm run compile-i18n-en && npm run compile-i18n-es && npm run compile-i18n-pt && npm run compile-i18n-pt-br",
    "compile-i18n-en": "formatjs compile lang/en.json --ast --out-file src/compiled-lang/en.json",
    "compile-i18n-es": "formatjs compile lang/es.json --ast --out-file src/compiled-lang/es.json",
    "compile-i18n-pt": "formatjs compile lang/pt.json --ast --out-file src/compiled-lang/pt.json",
    "compile-i18n-pt-br": "formatjs compile lang/pt-br.json --ast --out-file src/compiled-lang/pt-br.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "^4.4.4"
  }
}